<?xml version='1.2' encoding='utf-8'?>
<widget id="eu.natural.solutions.SauvagesDeMaRue" version="1.2" xmlns:android="http://schemas.android.com/apk/res/android" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>SauvagesDeMaRue</name>
    <description>
        Mobile application for "sauvage de ma rue" citizen science project
    </description>
    <author email="contact@natural-solutions.eu" href="http://www.natural-solutions.eu">
        Natural Solutions 
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="Orientation" value="portrait"/>
    <preference name="DisallowOverscroll" value="true"/>
    <preference name="SplashScreen" value="splash" />
    <preference name="SplashScreenDelay" value="10000" />

    <feature name="NetworkStatus">
        <param name="ios-package" value="CDVConnection" />
    </feature>
</widget>

